{
  "name": "@shopify/shopify-app-session-storage-test-utils",
  "version": "1.0.0-rc2",
  "description": "Utilities for testing Shopify App Session Storage",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shopify/shopify-app-js.git"
  },
  "bugs": {
    "url": "https://github.com/Shopify/shopify-app-js/issues"
  },
  "homepage": "https://github.com/Shopify/shopify-app-js/tree/main/packages/session-storage-test-utils",
  "author": "Shopify Inc.",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "prettier": "@shopify/prettier-config",
  "scripts": {
    "test:all": "jest",
    "test": "jest --selectProjects test_utils",
    "test:dev": "jest --selectProjects test_utils lint",
    "lint": "jest --selectProjects lint",
    "clean": "yarn tsc --build --clean",
    "prebuild": "yarn tsc --build --clean",
    "build": "tsc",
    "prepublishOnly": "yarn run build",
    "preversion": "yarn run test",
    "postversion": "git push origin main --follow-tags && echo \"Log in to shipit to deploy version $npm_package_version\""
  },
  "publishConfig": {
    "access": "restricted"
  },
  "keywords": [
    "shopify",
    "node",
    "app",
    "graphql",
    "rest",
    "webhook",
    "Admin API",
    "Storefront API",
    "session storage",
    "test utilities"
  ],
  "dependencies": {
    "@shopify/shopify-api": "^6.0.0-rc9",
    "@shopify/shopify-app-session-storage": "../shopify-app-session-storage/shopify-shopify-app-session-storage-1.0.0-rc1.tgz",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@shopify/eslint-plugin": "^42.0.1",
    "@shopify/prettier-config": "^1.1.2",
    "@types/jest": "^29.1.1",
    "eslint": "^8.24.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "jest-fetch-mock": "^3.0.3",
    "jest-runner-eslint": "^1.1.0",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "files": [
    "dist/*",
    "!bundle/*",
    "!tsconfig.tsbuildinfo",
    "!node_modules"
  ]
}
